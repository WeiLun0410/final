public class DailySales {
    private String salesID;
    private String salesDate;
    private Item item;
    private int quantity;

    private User user;

    public String getSalesID() {
        return salesID;
    }

    public String getSalesDate() {
        return salesDate;
    }

    public Item getItem() {
        return item;
    }

    public int getQuantity() {
        return quantity;
    }

    public User getUser() {
        return user;
    }

    public DailySales(String salesID,String salesDate, Item item, int quantity, User user) {
        this.salesDate = salesDate;
        this.item = item;
        this.quantity = quantity;
        this.user = user;
        this.salesID = salesID;
    }
}
